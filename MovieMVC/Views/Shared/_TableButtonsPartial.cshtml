@model int

<td style="width:150px;">
    <div class="btn-group" role="group">
        <a type="button" class="btn btn-primary" asp-action="Edit" asp-route-id="@Model">
            <i class="fas fa-edit"></i>
        </a>
        <a type="button" class="btn btn-info" asp-action="Details" asp-route-id="@Model">
            <i class="fas fa-list-alt"></i>

        </a>
        <a type="button" class="btn btn-danger" asp-action="Delete" asp-route-id="@Model">
            <i class="fas fa-trash-alt"></i>
        </a>
    </div>
</td>