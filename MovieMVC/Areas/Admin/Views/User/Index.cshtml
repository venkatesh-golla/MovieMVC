
@{
    ViewData["Title"] = "Users List";
}

<br />

<br />
<div class="p-4 border rounded" style="background-color:aliceblue;">
    <div class="row">
        <div class="col-sm-6">
            <h2 class="text-primary">Users List </h2>
        </div>
        <div class="col-sm-6 text-right">
            <a class="btn btn-primary" asp-area="Identity" asp-page="/Account/Register"><i class="fas fa-plus"></i> &nbsp; Create New User</a>
        </div>
    </div>

    <br />
    <div style="overflow-x:auto;">
        <table id="tblData" class="table table-striped table-bordered" style="width:100%">
            <thead class="thead-dark">
                <tr class="table-info">
                    <th>Name</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Compnay</th>
                    <th>Role</th>
                    <th></th>
                </tr>
            </thead>
        </table>
    </div>
</div>


@section Scripts{
    <script src="~/js/User.js"></script>
}